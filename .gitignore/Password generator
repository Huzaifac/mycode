import string 
import random
s1=string.ascii_lowercase
s2=string.ascii_uppercase
s3=string.digits
s4=string.punctuation
passlen=int(input("Enter the length of password \n"))
s=[]
s.extend(s1)
s.extend(s2)
s.extend(s3)
s.extend(s4)
password=random.sample(s,passlen)

print(f"Your password is\n", "".join(password))
